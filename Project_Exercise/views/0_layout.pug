  block scripts

Body
  Header
    // Navbar
    nav.navbar.navbar-expand-md.navbar.navbar-dark.bg-dark.fixed-top
      .container-fluid
        a.navbar-brand(href='/') Stadtführer Münster
        // Hamburger button
        button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarCollapse')
          span.navbar-toggler-icon
        // Navbar content
        #navbarCollapse.collapse.navbar-collapse
          .navbar-nav
            a.nav-item.nav-link.active(href='/') Stadttouren
            a.nav-item.nav-link.active(href='/edit') Sehenswürdigkeiten bearbeiten/Touren erstellen
          block search

  block content

  // Impressum footer
  footer.bg-light.text-center.text-lg-start
    // Grid container
    .container.p-4
      // Grid row
      .row
        // Grid column
        .col-lg-6.col-md-12.mb-4.mb-md-0
          h5.text-uppercase Stadtführer Münster
          p
            | Kurze Beschreibung der Website
        // Grid column
        #contact.col-lg-6.col-md-12.mb-4.mb-md-0
          h5.text-uppercase Kontakt
          p
            i.fa.fa-home.me-3
            |  M&uuml;nster, 48149, DE
          p
            i.fa.fa-envelope.me-3
            |  stadtfuehrer@stadt-muenster.de
          p
            i.fa.fa-phone.me-3
            |  + 01 234 567 88
          p
            i.fa.fa-print.me-3
            |  + 01 234 567 89
        // Grid column
      // Grid row
    // Grid container
